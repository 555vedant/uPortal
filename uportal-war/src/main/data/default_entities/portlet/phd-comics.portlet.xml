
<portlet-definition
    xmlns="https://source.jasig.org/schemas/uportal/io/portlet-definition" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://source.jasig.org/schemas/uportal/io/portlet-definition ../../../resources/xsd/io/portlet-definition/portlet-definition-4.0.xsd"
   
    version="4.0"
>

  <title>PHD Comics</title>
  <name>PHD Comics</name>
  <fname>phd-comics</fname>
  <desc>Latest PHD Comics strip</desc>
  <type>RSS</type>
  <timeout>10000</timeout>
  <portlet-descriptor>
    <webAppName>/NewsReaderPortlet</webAppName>
    <portletName>single-feed-news</portletName>
  </portlet-descriptor>
  
  
  
  
    <category>Entertainment</category>
  
  
    <group>Everyone</group>
  
  
	<parameter>
      <name>iconUrl</name>
      <value>/ResourceServingWebapp/rs/tango/0.8.90/32x32/emotes/face-smile.png</value>
    </parameter>
    <parameter>
        <name>mobileIconUrl</name>
        <value>/uPortal/media/skins/icons/mobile/feed.png</value>
    </parameter>
    <parameter>
        <name>editable</name>
        <value>true</value>
    </parameter>
    <parameter>
        <name>hasHelp</name>
        <value>true</value>
    </parameter>
  
  
    <portlet-preference> 
      <name>name</name>
      <value>PhD Comics</value>
    </portlet-preference>
    <portlet-preference> 
      <name>url</name>
      <value>http://www.phdcomics.com/gradfeed.php</value>
    </portlet-preference>
    <portlet-preference> 
      <name>summaryView</name>
      <value>full</value>
    </portlet-preference>
    <portlet-preference> 
      <name>maxStories</name>
      <value>1</value>
    </portlet-preference>
  
</portlet-definition>