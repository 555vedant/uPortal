<?xml version="1.0" encoding="UTF-8"?>
<portlet-definition
    xmlns="https://source.jasig.org/schemas/uportal/io/portlet-definition" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://source.jasig.org/schemas/uportal/io/portlet-definition ../../../resources/xsd/io/portlet-definition/portlet-definition-4.0.xsd"
   
    version="4.0"
>

  <title>About.com College Life</title>
  <name>About.com College Life</name>
  <fname>about-college-life</fname>
  <desc>About.com College Life RSS feed</desc>
  <type>RSS</type>
  <timeout>10000</timeout>
  <portlet-descriptor>
    <webAppName>/NewsReaderPortlet</webAppName>
    <portletName>single-feed-news</portletName>
  </portlet-descriptor>
  
    <category>Entertainment</category>
    <category>News</category>
  
  
    <group>Everyone</group>
  
  
    <parameter>
      <name>disableDynamicTitle</name>
      <value>true</value> 
    </parameter>
	<parameter>
      <name>iconUrl</name>
      <value>/ResourceServingWebapp/rs/tango/0.8.90/32x32/apps/internet-news-reader.png</value>
    </parameter>
  
    <portlet-preference> 
      <name>name</name>
      <value>About.com College Life</value>
    </portlet-preference>
    <portlet-preference> 
      <name>url</name>
      <value>http://z.about.com/6/g/collegelife/b/rss2.xml</value>
    </portlet-preference>
    <portlet-preference> 
      <name>summaryView</name>
      <value>flyout</value>
    </portlet-preference>
  
</portlet-definition>