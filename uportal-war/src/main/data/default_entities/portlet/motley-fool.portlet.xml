
<portlet-definition
    xmlns="https://source.jasig.org/schemas/uportal/io/portlet-definition" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://source.jasig.org/schemas/uportal/io/portlet-definition ../../../resources/xsd/io/portlet-definition/portlet-definition-4.0.xsd"
   
    version="4.0"
>

  <title>Motley Fool</title>
  <name>The Motley Fool</name>
  <fname>motley-fool</fname>
  <desc>Motley Fool channel</desc>
  <type>RSS</type>
  <timeout>10000</timeout>
  <portlet-descriptor xmlns:up="https://source.jasig.org/schemas/uportal">
    <up:webAppName>/NewsReaderPortlet</up:webAppName>
    <up:portletName>single-feed-news</up:portletName>
  </portlet-descriptor>
  
  
  
  
    <category>Entertainment</category>
  
  
    <group>Everyone</group>
  
  
    <parameter>
      <name>disableDynamicTitle</name>
      <value>true</value> 
    </parameter>
	<parameter>
      <name>iconUrl</name>
      <value>/ResourceServingWebapp/rs/tango/0.8.90/32x32/apps/internet-news-reader.png</value>
    </parameter>
  
  
    <portlet-preference> 
      <name>name</name>
      <value>Motley Fool</value>
    </portlet-preference>
    <portlet-preference> 
      <name>url</name>
      <value>http://www.fool.com/xml/foolnews_rss091.xml</value>
    </portlet-preference>
    <portlet-preference> 
      <name>summaryView</name>
      <value>flyout</value>
    </portlet-preference>
  
</portlet-definition>