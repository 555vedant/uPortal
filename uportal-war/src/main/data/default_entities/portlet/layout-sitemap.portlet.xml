
<portlet-definition
    xmlns="https://source.jasig.org/schemas/uportal/io/portlet-definition" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://source.jasig.org/schemas/uportal/io/portlet-definition ../../../resources/xsd/io/portlet-definition/portlet-definition-4.0.xsd"
    script="classpath://org/jasig/portal/io/jaxb-import_v4-0.crn"
    version="4.0"
>

  <title>Site map</title>
  <name>Site map</name>
  <fname>layout-sitemap</fname>
  <desc>A quick view of all your tabs and channels</desc>
  <type>Portlet</type>
  <timeout>30000</timeout>
  <portlet-descriptor>
    <isFramework>true</isFramework>
    <portletName>Sitemap</portletName>
  </portlet-descriptor>
  
  
  
  
    <category>uPortal</category>
  
  
    <group>Everyone</group>
  
  
    <parameter>
      <name>disableDynamicTitle</name>
      <value>true</value> 
    </parameter>
	<parameter>
      <name>iconUrl</name>
      <value>/ResourceServingWebapp/rs/tango/0.8.90/32x32/apps/preferences-system-windows.png</value>
    </parameter>
  
  
</portlet-definition>